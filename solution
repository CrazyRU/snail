A=int(input())
B=int(input())
L=int(input())
#Задаем переменные для дальнейших вычислений
X=0 #Отвечает за количество пройденного растояния
Y=0 #Отвечает за количество пройденных сторон
while X<L: 
    if Y%2 == 0:
        X=X+B
    else:
        X=X+A
    Z+=1 
if Z % 4 == 0:
    print("LEFT")
elif Z % 3 == 0:
    print("TOP")
elif Z % 2 == 0:
    print("RIGHT")
else:
    print("BOTTOM")
